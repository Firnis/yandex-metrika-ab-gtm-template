const injectScript = require('injectScript');
const createArgumentsQueue = require('createArgumentsQueue');
const log = require('logToConsole');

const ym = createArgumentsQueue('ym','ym.a');
const ymab = createArgumentsQueue('ymab','ymab.a');

const jsexp = 'https://abt.s3.mds.yandex.net/expjs/latest/exp.js';

injectScript(jsexp);

ymab(data.clientId, "init", function (answer) {
  ym(data.counterId, "experiments", answer.experiments);
});

data.gtmOnSuccess();


